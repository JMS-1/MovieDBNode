{
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false
    },
    "css.validate": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "files.associations": {
        "*.{overrides,variables}": "less"
    },
    "files.exclude": {
        "Server/src/**/*.js": true,
        "Server/src/**/*.js.map": true
    },
    "less.validate": false,
    "scss.validate": false,
    "stylelint.autoFixOnSave": true,
    "stylelint.enable": true,
    "typescript.tsdk": "node_modules/typescript/lib"
}